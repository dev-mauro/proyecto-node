<h1>Carrito</h1>

<table>
  <thead>
    <th>ID</th>
    <th>Nombre</th>
    <th>Descripcion</th>
    <th>Categoria</th>
    <th>Precio</th>
    <th>Thumbnails</th>
    <th>CÃ³digo</th>
    <th>Stock</th>
    <th>En el carro</th>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <td>{{this.product._id}}</td>
      <td>{{this.product.title}}</td>
      <td>{{this.product.description}}</td>
      <td>{{this.product.category}}</td>
      <td>{{this.product.price}}</td>
      <td>{{this.product.thumbnail}}</td>
      <td>{{this.product.code}}</td>
      <td>{{this.product.stock}}</td>
      <td>{{this.quantity}}</td>
    </tr>
    {{/each}}
  </tbody>

</table>